#!/usr/bin/env python3
"""
Command line interface to topiary.raxml.generate_ancestors
"""

import topiary
from topiary.external.raxml import find_best_model
from topiary._private import wrap_function

import sys

def main(argv=None):

    if argv is None:
        argv = sys.argv[1:]

    # Set arg types for args with None as default
    optional_arg_types = {}

    # Wrap and run function
    wrap_function(find_best_model,
                  argv=argv,
                  optional_arg_types=optional_arg_types)

if __name__ == "__main__":
    main()
